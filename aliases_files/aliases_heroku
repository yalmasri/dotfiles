# Heroku aliases
alias hga='heroku git:remote -a' # to add app to folder.
alias gph='git push heroku master'
alias hr='heroku run'
alias hdbr='heroku pg:reset DATABASE'
alias hdbm='heroku run rake db:migrate'
alias hdbs='heroku run rake db:seed'
alias hl='heroku logs --tail'
alias ho='heroku open'
alias hpstaging='git push staging $(branch_name):master'
alias hptesting='git push testing $(branch_name):master'
